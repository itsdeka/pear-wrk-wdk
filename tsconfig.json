{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "noEmit": true
  },
  "include": [
    "types/**/*.d.ts"
  ],
  "exclude": [
    "node_modules",
    "generated",
    "bundle",
    "test"
  ]
}

